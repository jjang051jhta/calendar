<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jjang051.calendar.dao.CalendarDao">
    <insert id="insertCalendar" parameterType="CalendarDto">
        insert into calendar values(calendar_seq.nextval,#{title},#{start},#{end},#{allDay})
    </insert>
    <select id="getAllTodo">
        select id,title, startdate as "start", enddate as "end" from calendar
    </select>
</mapper>